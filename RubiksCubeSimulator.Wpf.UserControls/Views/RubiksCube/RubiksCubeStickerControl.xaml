<UserControl x:Class="RubiksCubeSimulator.Wpf.UserControls.Views.RubiksCube.RubiksCubeStickerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:cubeViewModels="clr-namespace:RubiksCubeSimulator.Wpf.UserControls.ViewModels.RubiksCube"
             mc:Ignorable="d"
             d:DesignHeight="50" d:DesignWidth="50">

    <UserControl.DataContext>
        <cubeViewModels:RubiksCubeStickerControlViewModel />
    </UserControl.DataContext>

    <Grid Background="{Binding StickerColorBrush}"/>
</UserControl>